{
  "app": {
    "signingConfigs": [],
    "products": [
      {
        "name": "default",
        "signingConfig": "default",
        "compatibleSdkVersion": "5.0.0(12)",
        "runtimeOS": "HarmonyOS",
        "buildOption": {
          "strictMode": {
            "caseSensitiveCheck": true,
            "useNormalizedOHMUrl": true
          }
        }
      }
    ],
    "buildModeSet": [
      {
        "name": "debug",
      },
      {
        "name": "release"
      }
    ]
  },
  "modules": [
    {
      "name": "phone",
      "srcPath": "./products/phone",
      "targets": [
        {
          "name": "default",
          "applyToProducts": [
            "default"
          ]
        }
      ]
    },
    {
      "name": "home",
      "srcPath": "./feature/home"
    },
    {
      "name": "List",
      "srcPath": "./feature/List"
    },
    {
      "name": "login",
      "srcPath": "./feature/login"
    },
    {
      "name": "RouterModule",
      "srcPath": "./common/RouterModule"
    },
    {
      "name": "rdb",
      "srcPath": "./common/rdb",
    }
  ]
}